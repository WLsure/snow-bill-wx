

<view class="page">
    <view class="page__bd">
        <view class="weui-cells__title">账单列表</view>
        <view class="weui-cells weui-cells_after-title">
            <block wx:for="{{billlist}}" wx:key="{{index}}">
                <view class="weui-cell">
                    <view class="weui-cell__hd">
                        <image src="../../images/star.png" style="margin-right: 5px;vertical-align: middle;width:20px; height: 20px;"></image>
                    </view>
                    <view class="weui-cell__bd">{{item.name}}</view>
                    <view class="weui-cell__bd">{{item.buydate}}</view>
                    <view class="weui-cell__ft">{{item.money}}</view>
                </view>
            </block>
        </view>
    </view>
</view>